{
  "name": "basic",
  "template": "index.html",
  "baseDir": "../../",
  "savedDataUrl": "saved-data.json",
  "plugin": {
    "plugins": [
      {
        "type": "titles",
        "path": "{{baseDir}}templates/assets/plugins/titles/popcorn.titles.js"
      },
      {
        "type": "photo",
        "path": "{{baseDir}}templates/assets/plugins/photo/popcorn.photo.js"
      },
      {
        "type": "zoink",
        "path": "{{baseDir}}templates/assets/plugins/zoink/popcorn.zoink.js"
      },
      {
        "type": "looper",
        "path": "{{baseDir}}templates/assets/plugins/looper/popcorn.looper.js"
      },
      {
        "type": "barbara",
        "path": "{{baseDir}}templates/assets/plugins/barbara/popcorn.barbara.js"
      }
    ],
    "defaults": [
      "titles",
      "photo",
      "zoink",
      "looper",
      "barbara"
    ]
  },
  "editor": {
    "googlemap": "{{baseDir}}editors/googlemap-editor.js"
  },
  "maxPluginZIndex": 1000
}
